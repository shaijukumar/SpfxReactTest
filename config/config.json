{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "demo-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/demo/DemoWebPart.js",
          "manifest": "./src/webparts/demo/DemoWebPart.manifest.json"
        }
      ]
    },
    "portal-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/portal/PortalWebPart.js",
          "manifest": "./src/webparts/portal/PortalWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "DemoWebPartStrings": "lib/webparts/demo/loc/{locale}.js",
    "PortalWebPartStrings": "lib/webparts/portal/loc/{locale}.js"
  }
}
